{
  "name": "env-prompt",
  "version": "1.2.3",
  "description": "A dependency-free utility that prompts you for your project's environment variables.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node bin/build.ts --watch",
    "build": "ts-node bin/build.ts --build",
    "build:dist": "NODE_ENV=production ts-node bin/build.ts --build",
    "test:functional": "npx jest ./test/functional/test.ts",
    "test:unit": "npx jest ./test/unit",
    "release": "ts-node bin/release.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/env-prompt/env-prompt.git"
  },
  "keywords": [
    ".env",
    "env",
    "parameters",
    "environment",
    "variables",
    "configuration",
    "config",
    "interactive",
    "install"
  ],
  "readmeFilename": "README.md",
  "bin": {
    "env-prompt": "./dist/index.js"
  },
  "author": "Brian Kotos <brian.kotos@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.11.2",
    "chokidar": "^3.5.2",
    "esbuild": "^0.12.21",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.0"
  }
}
